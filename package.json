{
  "name": "archipelabot",
  "version": "0.9.0",
  "description": "Manages Archipelago instances and required YAML config files over Discord.",
  "author": {
    "name": "KewlioMZX"
  },
  "files": [
    "./bin/*",
    "./lib/*"
  ],
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "start": "node -r dotenv/config ./lib/index.js",
    "build": "tsc",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "bstart": "yarn build && yarn start",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "discord.js": "^14.9.0",
    "dotenv": "^16.3.1",
    "mkfifo": "^2.1.0",
    "sequelize": "^6.33.0",
    "sqlite3": "^5.1.6",
    "typescript": "^4.8.3",
    "yaml": "^2.3.3"
  },
  "devDependencies": {
    "@types/adm-zip": "^0",
    "@types/eslint": "^8",
    "@types/node": "^18",
    "@types/prettier": "^2",
    "@types/validator": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@yarnpkg/sdks": "^3.0.0-rc.53",
    "eslint": "^8.22.0",
    "prettier": "^2.7.1"
  },
  "resolutions": {
    "@sapphire/shapeshift": "^3.6.0"
  },
  "packageManager": "yarn@3.6.4"
}
